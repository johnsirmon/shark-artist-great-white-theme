  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8799a8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#9ec8bf"
      }
    },
    {
      "name": "String Escape / Regexp",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#88bdb5"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#c9a96a"
      }
    },
    {
      "name": "Boolean / Language Constant",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#c9a96a"
      }
    },
    {
      "name": "Keyword / Storage",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#4f7ea8"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#6c92b3"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#6f7f8a"
      }
    },
    {
      "name": "Function / Method Name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#9dc4ea"
      }
    },
    {
      "name": "Type / Class / Namespace",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.enum",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#8fa9bf"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#e7edf2"
      }
    },
    {
      "name": "Language Variable (this / self / super)",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#9dc4ea",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#c26b73"
      }
    },
    {
      "name": "Decorator / Annotation",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "storage.type.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#c26b73"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#4f7ea8"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9ec8bf"
      }
    },
    {
      "name": "Tag Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#6f7f8a"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#9ec8bf"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#bac9d4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#e7edf2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#c9d5df",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#9ec8bf"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#9dc4ea",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Blockquote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#8799a8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#c44f5f",
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#e7edf2",
    "variable.readonly": "#c26b73",
    "parameter": "#c9d5df",
    "property": "#9dc4ea",
