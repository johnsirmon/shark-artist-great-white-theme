  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4e6472",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#0e6f5f"
      }
    },
    {
      "name": "String Escape / Regexp",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#0f6869"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#6c4f0f"
      }
    },
    {
      "name": "Boolean / Language Constant",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#6c4f0f"
      }
    },
    {
      "name": "Keyword / Storage",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#0f5f93"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#266f9f"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#7a909a"
      }
    },
    {
      "name": "Function / Method Name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#005784"
      }
    },
    {
      "name": "Type / Class / Namespace",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.enum",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#2d5167"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#12202a"
      }
    },
    {
      "name": "Language Variable (this / self / super)",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#005784",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#8a3848"
      }
    },
    {
      "name": "Decorator / Annotation",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "storage.type.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#8a3848"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#0f5f93"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0e6f5f"
      }
    },
    {
      "name": "Tag Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#7a909a"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#0e6f5f"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#324856",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#12202a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#395262",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#0e6f5f"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#005784",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Blockquote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#4e6472",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#c44f5f",
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#12202a",
    "variable.readonly": "#8a3848",
    "parameter": "#315b74",
    "property": "#2e5970",
    "property.readonly": "#8a3848",
    "function": "#005784",
    "method": "#005784",
    "class": "#2d5167",
    "interface": "#2d5167",
    "enum": "#2d5167",
    "enumMember": "#8a3848",
    "type": "#2d5167",
    "typeParameter": "#2d5167",
    "namespace": "#2d5167",
    "keyword": "#0f5f93",
    "modifier": "#0f5f93",
    "operator": "#266f9f",
    "decorator": "#8a3848",
    "string": "#0e6f5f",
    "number": "#6c4f0f",
    "regexp": "#0f6869"
  }
}
