  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#728fa0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#6ec4ac"
      }
    },
    {
      "name": "String Escape / Regexp",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#5cbdba"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#d4a843"
      }
    },
    {
      "name": "Boolean / Language Constant",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#d4a843"
      }
    },
    {
      "name": "Keyword / Storage",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#4a9ec0"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#5b98b0"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#486878"
      }
    },
    {
      "name": "Function / Method Name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#8dd4f0"
      }
    },
    {
      "name": "Type / Class / Namespace",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.enum",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#78b0c8"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#d8e8ef"
      }
    },
    {
      "name": "Language Variable (this / self / super)",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#8dd4f0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#d9a441"
      }
    },
    {
      "name": "Decorator / Annotation",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "storage.type.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#d9a441"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#4a9ec0"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#6ec4ac"
      }
    },
    {
      "name": "Tag Punctuation",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#486878"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#6ec4ac"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#9ec4d8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#d8e8ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#b5d0e0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#6ec4ac"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#8dd4f0",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Blockquote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#728fa0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#c44f5f",
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#d8e8ef",
    "variable.readonly": "#d9a441",
    "parameter": "#b5d0e0",
    "property": "#98c4d8",
    "property.readonly": "#d9a441",
    "function": "#8dd4f0",
    "method": "#8dd4f0",
    "class": "#78b0c8",
