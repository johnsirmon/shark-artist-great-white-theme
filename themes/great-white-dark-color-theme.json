    "keyword": "#4a9ec0",
    "modifier": "#4a9ec0",
    "operator": "#5b98b0",
    "decorator": "#d9a441",
    "string": "#6ec4ac",
    "number": "#d4a843",
    "regexp": "#5cbdba"
  }
